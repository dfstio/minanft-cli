{
  "name": "minanft-cli",
  "description": "Mina NFT CLI tool",
  "keywords": [
    "minanft",
    "NFT",
    "cli",
    "offline",
    "mina",
    "zkapp",
    "zk",
    "blockchain",
    "web3",
    "zero knowledge"
  ],
  "version": "1.0.4",
  "main": "index.js",
  "author": "dfst.io",
  "license": "Apache-2.0",
  "scripts": {
    "cli": "ts-node src/cli.ts",
    "test": "jest --silent",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/cli-progress": "^3.11.5",
    "axios": "^1.6.3",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "minanft": "^1.3.34",
    "o1js": "^0.15.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "slowTestThreshold": 40,
    "verbose": true
  },
  "bin": {
    "minanft": "./src/cli.ts"
  },
  "packageManager": "yarn@4.0.2"
}
